@model Slider
<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
           <div class="col-4">
                <img style="width:250px; height:250px" src="~/assets/images/website-images/@Model.ImageURL"></img>
           </div>
            <div class="col-8">
                <p class="title">Title: @Model.Title</p>
                <p>Offer: @Model.Offer</p>
                <p>Description: @Model.Description</h3>
                <p>Button: @Model.Button</p>
                <p>Order: @Model.Order</h3>
            </div>
        </div>
    </div>
</div>

